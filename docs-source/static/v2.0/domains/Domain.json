        },
        "serverName": {
          "description": "WebLogic server name. Required",
          "type": "string"
        },
        "health": {
          "description": "Current status and health of a specific WebLogic server.",
          "$ref": "#/definitions/ServerHealth"
        },
        "state": {
          "description": "Current state of this WebLogic server. Required",
          "type": "string"
        }
      }
