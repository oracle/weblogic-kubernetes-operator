    "Map": {
      "type": "object",
      "properties": {}
    },
    "ProbeTuning": {
      "type": "object",
      "properties": {
        "periodSeconds": {
          "description": "The number of seconds between checks",
          "type": "number"
        },
        "timeoutSeconds": {
          "description": "The number of seconds with no response that indicates a failure",
          "type": "number"
        },
        "initialDelaySeconds": {
          "description": "The number of seconds before the first check is performed",
          "type": "number"
        }
      }
    },
    "ServerHealth": {
