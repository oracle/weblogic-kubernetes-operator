<?xml version='1.0' encoding='UTF-8'?>
<jdbc:jdbc-data-source
    xmlns:jdbc="http://xmlns.oracle.com/weblogic/jdbc-data-source"
    xmlns:jdbcf="http://xmlns.oracle.com/weblogic/jdbc-data-source-fragment"
    xmlns:s="http://xmlns.oracle.com/weblogic/situational-config" >
    <jdbc:name >JdbcTestDataSource-1</jdbc:name>
    <jdbc:datasource-type>GENERIC</jdbc:datasource-type>
    <jdbc:jdbc-connection-pool-params>
        <jdbc:initial-capacity jdbcf:combine-mode="replace">5</jdbc:initial-capacity>
        <jdbc:max-capacity jdbcf:combine-mode="replace">10</jdbc:max-capacity>
        <jdbc:min-capacity jdbcf:combine-mode="replace">6</jdbc:min-capacity>
        <jdbc:ignore-in-use-connections-enabled jdbcf:combine-mode="replace">true</jdbc:ignore-in-use-connections-enabled>
        <jdbc:login-delay-seconds jdbcf:combine-mode="replace">10</jdbc:login-delay-seconds>
        <jdbc:statement-cache-type jdbcf:combine-mode="replace">FIXED</jdbc:statement-cache-type>
    </jdbc:jdbc-connection-pool-params>
    <jdbc:jdbc-data-source-params>
        <jdbc:global-transactions-protocol jdbcf:combine-mode="replace">EmulateTwoPhaseCommit</jdbc:global-transactions-protocol>
    </jdbc:jdbc-data-source-params>
</jdbc:jdbc-data-source>
